{
  "name": "zerospam/sdk-framework",
  "description": "Framework to build SDK",
  "license": "MIT",
  "authors": [
    {
      "name" : "Phil"
    }
  ],
  "keywords": [
    "sdk",
    "framework"
  ],
  "require": {
    "php": "^8.0",
    "nesbot/carbon": "^1.25 || ^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "marc-mabe/php-enum": "^3.0 || ^4.0",
    "league/oauth2-client": "^2.3",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.2",
    "php-coveralls/php-coveralls": "^2.0",
    "phploc/phploc": "^7.0",
    "dms/phpunit-arraysubset-asserts": "^v0.3.0"
  },
  "autoload": {
    "psr-4": {
      "ZEROSPAM\\Framework\\SDK\\": "src/",
      "ZEROSPAM\\Framework\\SDK\\Test\\Base\\": "tests/src/Base"
    },
    "files": [
      "src/Utils/Helpers/array.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ZEROSPAM\\Framework\\SDK\\Test\\Tests\\": "tests/src/Tests"
    }
  }
}
