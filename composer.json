{
  "name": "zerospam/sdk-framework",
  "description": "Framework to build SDK",
  "license": "MIT",
  "authors": [
    {
      "name": "Antoine Aflalo",
      "email": "dev.api@zerospam.ca",
      "homepage": "https://www.zerospam.ca",
      "role": "Project founder"
    },
    {
      "name": "Philippe Bourdeau",
      "email": "dev.api@zerospam.ca",
      "homepage": "https://www.zerospam.ca",
      "role": "Developer"
    },
